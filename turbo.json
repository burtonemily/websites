{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_ENV_FLAG"
  ],
  "tasks": {
    "build:test": {
      "dependsOn": [
        "^build:test"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.local"
      ],
      "env": [
        "NEXT_PUBLIC_SENT_STAKING_API_URL",
        "NEXT_PUBLIC_SENT_EXPLORER_API_URL",
        "NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID",
        "FAUCET_WALLET_PRIVATE_KEY",
        "FAUCET_DB_SECRET_KEY",
        "FAUCET_HOURS_BETWEEN_USES",
        "FAUCET_CHAIN",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_CLIENT_ID",
        "TELEGRAM_BOT_TOKEN",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-telemetry": {
      "cache": false
    },
    "test": {}
  }
}
